// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`Winglang Github snapshot 1`] = `
"# ~~ Generated by projen. To modify, edit .projenrc.js and run \\"npx projen\\".

name: deploy
on:
  push:
    branches:
      - main
  workflow_dispatch: {}
jobs:
  compile:
    runs-on: ubuntu-latest
    permissions:
      contents: read
    steps:
      - name: Checkout
        uses: actions/checkout@v4
      - name: Install Winglang CLI
        run: npm install -g winglang
      - name: Build
        run: wing compile - t tf-aws main.w
      - name: Upload Wing compile artifact
        uses: actions/upload-artifact@v4
        with:
          name: wing-compile-artifact
          path: target
  deploy-dev:
    needs: compile
    runs-on: ubuntu-latest
    permissions:
      contents: read
      id-token: write
    steps:
      - name: Download Wing compile artifact
        uses: actions/download-artifact@v4
        with:
          name: wing-compile-artifact
          path: target
      - uses: aws-actions/configure-aws-credentials@v4
        with:
          aws-region: eu-central-1
          role-to-assume: devRole
          role-session-name: DeploySession-dev
      - name: Terrform Init
        run: terraform init
        working-directory: target/main.tfaws
      - name: Deploy
        run: terraform apply -auto-approve
        working-directory: target/main.tfaws
  deploy-prod:
    needs:
      - compile
      - deploy-dev
    runs-on: ubuntu-latest
    permissions:
      contents: read
      id-token: write
    steps:
      - name: Download Wing compile artifact
        uses: actions/download-artifact@v4
        with:
          name: wing-compile-artifact
          path: target
      - uses: aws-actions/configure-aws-credentials@v4
        with:
          aws-region: eu-central-1
          role-to-assume: prodRole
          role-session-name: DeploySession-prod
      - name: Terrform Init
        run: terraform init
        working-directory: target/main.tfaws
      - name: Deploy
        run: terraform apply -auto-approve
        working-directory: target/main.tfaws
"
`;
